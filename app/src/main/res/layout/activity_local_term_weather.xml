<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:weightSum="4"
            app:layout_constraintTop_toTopOf="@+id/guideline14" android:id="@+id/linearLayout2">
        <Button
                android:text="기상정보"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:layout_weight="1" android:id="@+id/btn_temp"/>
        <Button
                android:text="오늘의 조언"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:layout_weight="1" android:id="@+id/btn_advice"/>
        <Button
                android:text="상세일기\n예보"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:layout_weight="1" android:id="@+id/btn_detail"/>
        <Button
                android:text="지역검색"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:layout_weight="1" android:id="@+id/btn_localSearch"/>
    </LinearLayout>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/imageView17"
            app:srcCompat="@drawable/framesmall" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="100dp" app:layout_constraintBottom_toTopOf="@+id/imageView16"
            android:layout_marginStart="100dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline9"
            android:layout_marginTop="8dp" android:scaleType="fitXY"
    />
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/imageView16"
            app:srcCompat="@drawable/frame" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" android:scaleType="fitXY"
            app:layout_constraintBottom_toTopOf="@+id/guideline14"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintTop_toBottomOf="@+id/textView13"
            android:layout_marginTop="8dp" android:layout_marginBottom="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/imageView12"
            app:srcCompat="@drawable/framesmall" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="80dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="80dp"
            app:layout_constraintTop_toTopOf="@+id/guideline15"
            app:layout_constraintBottom_toTopOf="@+id/guideline10" android:scaleType="fitXY"
    />
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/imageView11"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline10"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline9"
            app:srcCompat="@drawable/frame" android:scaleType="fitXY"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:id="@+id/imageView9" app:srcCompat="@drawable/framesmall"
            android:scaleType="fitXY" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="60dp" android:layout_marginTop="76dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="60dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/imageView7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline16"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="@id/guideline15"
            android:scaleType="fitXY" app:srcCompat="@drawable/frame"/>

    <android.support.v7.widget.RecyclerView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toEndOf="@+id/include"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/guideline15" android:id="@+id/rv_3"
            app:layout_constraintTop_toTopOf="@+id/guideline16" android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"/>
    <ProgressBar
            style="?android:attr/progressBarStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/loading"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:visibility="invisible" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="36dp"
            android:fontFamily="sans-serif"
            android:textColor="@android:color/background_dark"
            android:textStyle="bold"
            android:id="@+id/inside_Title5"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:text="상세일기예보"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline5"/>

    <android.support.constraint.Guideline
            android:id="@+id/guideline5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.1" />
    <TextView
            android:text="3시간 일기예보"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7" android:textSize="34dp"
            android:textColor="#000000"
            app:layout_constraintStart_toStartOf="@+id/imageView9"
            app:layout_constraintEnd_toEndOf="@+id/imageView9"
            app:layout_constraintTop_toTopOf="@+id/imageView9" android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_bias="0.45" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/rv_3"/>
    <TextView
            android:text="주간 일기예보"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView22" android:textSize="34dp"
            android:textColor="#000000"
            app:layout_constraintStart_toStartOf="@+id/imageView12"
            app:layout_constraintEnd_toEndOf="@+id/imageView12" android:layout_marginEnd="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline15"
            app:layout_constraintHorizontal_bias="0.53"
            app:layout_constraintBottom_toBottomOf="@+id/imageView12" app:layout_constraintVertical_bias="0.48000002"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline7"
                                          android:orientation="vertical"
                                          app:layout_constraintGuide_percent="0.25"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline10"
                                          android:orientation="horizontal"
                                          app:layout_constraintGuide_percent="0.48"/>

    <include android:layout_width="wrap_content" android:layout_height="0dp" layout="@layout/detail_weather_info"
             app:layout_constraintTop_toTopOf="@+id/guideline10"
             app:layout_constraintBottom_toTopOf="@+id/guideline9" app:layout_constraintStart_toStartOf="parent"
             android:id="@+id/include2" android:layout_marginBottom="8dp" android:layout_marginTop="8dp"
             android:layout_marginStart="8dp"/>
    <android.support.v7.widget.RecyclerView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/guideline10" app:layout_constraintStart_toEndOf="@+id/include"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/guideline9"

            android:id="@+id/rv_week"
            android:layout_marginBottom="8dp" android:layout_marginTop="8dp"/>

    <android.support.constraint.Guideline
            android:id="@+id/guideline9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.69" />
    <TextView
            android:text="일출몰정보"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView13" app:layout_constraintStart_toStartOf="@+id/guideline7"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="@+id/imageView17"
            android:layout_marginEnd="8dp"
            android:textSize="34dp" android:textColor="#000000"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline9"
            app:layout_constraintHorizontal_bias="0.51" android:textAlignment="center"/>
    <TextView
            android:text="Loading.."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_sunRise" android:textColor="#FF0000"
            android:textSize="24dp" android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView13" android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintStart_toEndOf="@+id/textView16"
            app:layout_constraintEnd_toStartOf="@+id/guideline17"/>
    <TextView
            android:text="Loading.."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_sunDown" android:textColor="#FF0000"
            android:textSize="24dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView13" android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintStart_toEndOf="@+id/textView17"/>
    <TextView
            android:text="일출"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView16" android:textColor="#000000"
            android:textSize="24dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="4dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView13" android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="0.095" android:layout_marginEnd="8dp"/>
    <TextView
            android:text="일몰"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView17" android:textColor="#000000"
            android:textSize="24dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView13" android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="0.228" app:layout_constraintStart_toStartOf="@+id/guideline17"/>
    <TextView
            android:text="Loading.."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_moonDown" android:textColor="#FF0000"
            android:textSize="24dp" android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_sunRise" android:layout_marginTop="12dp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintStart_toEndOf="@+id/textView16"
            app:layout_constraintEnd_toStartOf="@+id/guideline17"/>
    <TextView
            android:text="Loading.."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_moonRise" android:textColor="#FF0000"
            android:textSize="24dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_sunDown" android:layout_marginTop="12dp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintStart_toEndOf="@+id/textView17"/>
    <TextView
            android:text="월출"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/월몰" android:textColor="#000000"
            android:textSize="24dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="4dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView16" android:layout_marginTop="12dp"
            app:layout_constraintHorizontal_bias="0.092" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_moonDown"/>
    <TextView
            android:text="월몰"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/월출" android:textColor="#000000"
            android:textSize="24dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView17" android:layout_marginTop="12dp"
            app:layout_constraintHorizontal_bias="0.228" app:layout_constraintStart_toStartOf="@+id/guideline17"/>
    <include android:layout_width="wrap_content" android:layout_height="0dp" layout="@layout/detail_weather_three"
             app:layout_constraintBottom_toTopOf="@+id/guideline15" app:layout_constraintStart_toStartOf="parent"
             android:id="@+id/include" android:layout_marginBottom="8dp"
             app:layout_constraintTop_toTopOf="@+id/guideline16" android:layout_marginTop="8dp"
             android:layout_marginStart="8dp"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline15"
                                          android:orientation="horizontal"
                                          app:layout_constraintGuide_percent="0.4"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline16"
                                          android:orientation="horizontal"
                                          app:layout_constraintGuide_percent="0.18"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline14"
                                          android:orientation="horizontal"
                                          app:layout_constraintGuide_percent="0.92612857"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline17"
                                          android:orientation="vertical"
                                          app:layout_constraintGuide_percent="0.5"/>

</android.support.constraint.ConstraintLayout>